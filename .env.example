# Autonomous Supply Chain - Environment Configuration
# Copy to .env and fill in your values, or run: sh setup.sh

# Vision Agent - Gemini API Key
# Get yours at: https://aistudio.google.com/apikey
GEMINI_API_KEY=your-gemini-api-key

# Supplier Agent - GCP Project
GOOGLE_CLOUD_PROJECT=your-gcp-project-id

# AlloyDB Connection
# Fill these in from your easy-alloydb-setup output
ALLOYDB_REGION=us-central1
ALLOYDB_CLUSTER=my-alloydb-cluster
ALLOYDB_INSTANCE=my-alloydb-instance
# This URI is built from the fields above â€” update it if you change them
ALLOYDB_INSTANCE_URI=projects/${GOOGLE_CLOUD_PROJECT}/locations/${ALLOYDB_REGION}/clusters/${ALLOYDB_CLUSTER}/instances/${ALLOYDB_INSTANCE}
DB_USER=postgres
DB_PASS=your-alloydb-password
DB_NAME=postgres
